# Auto detect text files and perform LF normalization
* text=auto

# Custom for Visual Studio
*.cs     diff=csharp

# Standard to msysgit
*.doc	 diff=astextplain
*.DOC	 diff=astextplain
*.docx diff=astextplain
*.DOCX diff=astextplain
*.dot  diff=astextplain
*.DOT  diff=astextplain
*.pdf  diff=astextplain
*.PDF	 diff=astextplain
*.rtf	 diff=astextplain
*.RTF	 diff=astextplain
git.lfs filter=lfs diff=lfs merge=lfs -text
git filter=lfs diff=lfs merge=lfs -text
CONTRIBUTING filter=lfs diff=lfs merge=lfs -text
README filter=lfs diff=lfs merge=lfs -text
html filter=lfs diff=lfs merge=lfs -text
build.xml filter=lfs diff=lfs merge=lfs -text
WEB-INF filter=lfs diff=lfs merge=lfs -text
jsp filter=lfs diff=lfs merge=lfs -text
css filter=lfs diff=lfs merge=lfs -text
jar filter=lfs diff=lfs merge=lfs -text
Guestbook filter=lfs diff=lfs merge=lfs -text
